{
    "make": "Archos", 
    "model": "3 Vision", 
    "description": "Xvid + MP3 for archos", 
    "version": "1.0", 
    "author": {
        "name": "H. Dussuel", 
        "email": "dbkblk@gmail.com"
    }, 
    "icon": "file://archos3vision.png",
    "default": "Normal", 
    "presets": [
        {
            "name": "Normal", 
            "extension": "avi", 
            "container": "avimux", 
            "vcodec": {
                "name": "xvidenc", 
                "container": "avimux", 
                "width": [
                    550, 550
                ], 
                "height": [
                    310, 310
                ], 
                "rate": [
                    25, 25
                ], 
                "passes": [
                    "pass=quant quantizer=8 max-bframes=0 trellis=true"
                ]
            }, 
            "acodec": {
                "name": "lame", 
                "container": "", 
                "width": [
                    8, 32
                ], 
                "depth": [
                    8, 24
                ], 
                "rate": [
                    8000, 96000
                ], 
                "channels": [
                    1, 2
                ], 
                "passes": [
                    "bitrate=128"
                ]
            }
        }
    ]
}
